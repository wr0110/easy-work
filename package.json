    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/parser": "4.15.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "dependency-cruiser": "^11.14.2",
    "dotenv-webpack": "^7.1.0",
    "eslint": "7.10.0",
