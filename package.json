{
  "name": "kanban-tasks",
  "sideEffects": "false",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack server",
    "build": "cross-env NODE_ENV=production webpack",
    "build:stats": "cross-env NODE_ENV=production webpack --profile --json > stats.json",
    "lint": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --write \"./src/**/**.{ts,tsx,js,jsx,json}\"",
    "commit": "git-cz",
    "test": "jest",
    "test:clear": "jest --clearCache",
    "deps": "depcruise --config .dependency-cruiser.js src",
    "deps:graph": "depcruise -c .dependency-cruiser.js --exclude \"node_modules/*\" --output-type dot src | dot -T svg > dependency-graph.svg"
  },
  "dependencies": {
    "@dnd-kit/core": "^5.0.1",
